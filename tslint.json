{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "rules": {
      "indent": [
        true,
        "spaces",
        2
      ],
      "quotemark": [
        true,
        "single"
      ],
      "semicolon": [
        true,
        "always"
      ],
      "no-console": false,
      "no-debugger": true
    },
    "linterOptions": {
      "exclude": [
        "node_modules/**"
      ]
    }
  }
  